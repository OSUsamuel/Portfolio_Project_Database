        <p><strong>Publishers</strong></p>
        <table id="publishers_table">
            <tr>
                <th>Publisher ID</th>
                <th>Name</th>
                <th>Website</th>
            </tr>
            <th>
                {{@key}}
            </th>
         

             {{#each data}}
        <tr data_value={{this.publisherID}}>
