 <main>

      <div class="topnav">
        <a class="active" href="index.html">Home</a>
        <a href="books_page.html">Books</a>
      </div>

    <p>&nbsp;</p>
    <div >
        <p><strong>Our Members</strong></p>
        <table id="members_table">
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Email</th>
            <th>
                {{@key}}
            </th>
         

             {{#each data}}
        <tr>
            <td>{{this.memberID}}</td>
            <td>{{this.name}}</td>
            <td>{{this.email}}</td>
        </tr>
        {{/each}}
        </table>
    </div>


    <p>&nbsp;</p>
    <div id="insert" style="display: block">
        <form id="addMemberFormAjax">
            <legend><strong>Add Member</strong></legend>
              <fieldset class="fields">
                <label for="firstName_input"> First Name </label> <input id="firstName_input" type="text" name="firstName">
                <label for="lastName_input"> Last Name</label> <input id="lastName_input" type="text" name = "lastName">
                <label for="email_input"> Email </label> <input id="email_input" type="email" name="email_input">
              </fieldset>
              <input type="submit" >
        </form> <!--add new member-->
    </div><!-- insert -->
    <script src="./js/add_member.js"></script>
    
    <p>&nbsp;</p>
    <div id="delete" style="display: block">
        <form id="deleteMember">
            <legend><strong>Delete Member</strong></legend>
              <fieldset class="fields">
                <label>Enter Member ID</label> <input type="text" name="memberID">
              </fieldset>
              <input class="btn" type="submit" id="deleteMember" value="Delete a Member">
              <input class="btn" type="button" value="cancel">
        </form> <!--add new member-->
    </div><!-- insert -->

    <p>&nbsp;</p>
    <div id="update" style="display: block">
        <form id="updateMember">
            <legend><strong>Update member</strong></legend>
                <fieldset class="fields">
                  <label> Name </label> <input type="text"  name="name">
                  <label> Email </label> <input type="email" name="email">
                  </fieldset>
          <input class="btn" type="submit" id="UpdateSaveMember" value="Save Update Member">
                <input class="btn" type="button" value="cancel" >
        </form> 
      </div><!-- update -->




  
    

    </main>